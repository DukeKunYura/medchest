{
  "expo": {
    "name": "MedChest",
    "slug": "MedChest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconMedChest.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9BAF64"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dukekunyura.medchest-rn-app"
    },
    "android": {
      "package": "com.dukekunyura.medchest_rn_app",
      "versionCode": 1,
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}